/* pages/getcard/getcard.wxss */
.top{text-align: center;}
.top .img{width: 168rpx;height: 162rpx;position: relative;margin: 0 auto 22rpx;padding: 24rpx 0;}
.top .portrait{width: 136rpx;height: 136rpx;border: 6rpx solid #fff;box-shadow: 0 6rpx 6rpx rgba(31, 156, 173, 0.1);}
.top .img::before,.top .img::after{content: '';display: block;position: absolute;background-position: center center;background-repeat: no-repeat;background-size: 100% auto;}
.top .img::before{top: 0;right: 0;width: 47rpx;height: 43rpx;background-image: url(data:image/png;base64,);}
.top .img::after{bottom: 0;left: 50%;width: 83rpx;height: 24rpx;margin-left: -41rpx;background-image: url(data:image/png;base64,);}
.top .h1,.top .h2{padding-bottom: 6rpx;}
.top .h1{font-style: italic;font-size: 40rpx;}
.top .h2{font-size: 30rpx;color: #999;}
.getcard{margin: 36rpx 24rpx;height: 98rpx;line-height: 98rpx;background: #21aa9f;box-shadow: 0 0 24rpx rgba(31, 156, 173, 0.6);}

.coupon-item::before{background-color: #fff;}
.coupon-item .money,.coupon-item .code{display: none;}
.coupon-item .types{font-size: 68rpx;line-height: 100rpx;}