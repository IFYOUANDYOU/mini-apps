/* pages/advertising/advertising.wxss */
.container{overflow: hidden;}
.floors{position: fixed;top: 35%;right: 24rpx;z-index: 9;width: 58rpx;line-height: 80rpx;background: #b2aeaa;color: #fff;text-align: center;overflow: hidden;margin-top: -150rpx;}
.floors .cur{background: #35beab;}
.mapImg{overflow: scroll;}
.mapImg .image{height: 820rpx;}
.bottom{padding: 0 24rpx;background: #fff;position: fixed;bottom: 0;left: 0;right: 0;z-index: 9;overflow: hidden;font-size: 30rpx;}
.points{margin: 20rpx;display: flex;align-items: center;justify-content: space-between;}
.points .point{width: 50%;height: 78rpx;background-color: #e9f0ef;color: #666;line-height: 78rpx;background-position: 24rpx center;}
.points .point,.path .li{background-repeat: no-repeat;background-size: 16rpx 16rpx;}
.points .point text{margin-left: 60rpx;}
.path{color: #999;padding: 0 10rpx;line-height: 44rpx;height: 164rpx;}
.path .li{margin: 8rpx 0;background-position: 0 center;padding-left: 32rpx;background-image: url(data:image/png;base64,);}
.points .start,.path .start{background-image: url(data:image/png;base64,);}
.points .end,.path .end{background-image: url(data:image/png;base64,);}
.points .boundary{width: 40rpx;height: 8rpx;margin: 0 24rpx;}
.guide{margin: 20rpx 20rpx 34rpx;background:-webkit-gradient(linear, left top, right bottom, from(#26a6bf), to(#07b6a5));color: #fff;line-height: 88rpx;text-align: center;box-shadow: 6rpx 6rpx 10rpx rgba(0, 0, 0, 0.3);font-size: 32rpx;}
.route{padding-top: 24rpx;}
.recommend,.recommend .title,.route,.route .title{margin: 0 10rpx 10rpx;}
.recommend .title,.route .title{font-size: 40rpx;}
.recommend .res{display: flex;flex-wrap: wrap;}
.recommend .res text{display: block;width: 30.33%;margin: 10rpx;line-height: 68rpx;background: #e9f0ef;color: #666;text-align: center;flex-grow: 1;}
/*切换下一段路线*/
.mapImg .elevator{position: fixed;background: rgba(0, 0, 0, 0.3);color: #fff;bottom: 460rpx;left: 50%;margin: 0 0 0 -105rpx;line-height: 78rpx;padding: 0 24rpx;font-size: 32rpx;}