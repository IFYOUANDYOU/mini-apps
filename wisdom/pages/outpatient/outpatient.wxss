.container{width: 70%;margin: 0 auto;}
.tt{font-size: 52rpx;text-align: center;overflow: hidden;border-bottom: 2rpx dashed #e1e1e1;padding: 22rpx 0;}
.tt text{display: block;font-size: 32rpx;color: #999;margin: 12rpx 0;}
form{padding: 24rpx;}
.item{overflow: hidden;margin: 32rpx 0;}
.item .input{background: #e8e8e8;line-height: 88rpx;height: 88rpx;padding: 0 24rpx;text-align: center;font-size: 30rpx;color: #999;}
.item .btn{margin-top: 60rpx;}
.tag{background: #2da3b3;text-align: center;padding: 100rpx 0;position: fixed;bottom: 0;left: 0;right: 0;}
.tag image{width: 356rpx;height: 82rpx;}

.goEval{}

.pickerView{position: fixed;bottom: 0;left: 0;width: 100%;z-index: 9;text-align: center;}
.pickerView .bj{position: fixed;top: 0;left: 0;background: rgba(0, 0, 0, 0.4);width: 100%;height: 100%;z-index: -1;}
picker-view-column{background: #fff;}
.pickerView .btns{line-height: 80rpx;overflow: hidden;border-bottom: 2rpx solid #f0f0f0;padding: 0 24rpx;background: #fff;}
.pickerView .btns text{display: block;}
.pickerView .btns .confirm{color: #07b6a5;}